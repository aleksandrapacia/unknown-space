#:import MapView kivy.garden.mapview.MapView

<TrackScreen>:
    name: 'iss_tracking'
    id: iss_tracking
    MapView:
        id: map
        lat: 10
        lon: 10
        zoom: 2
        double_tap_zoom: True
      
    MDLabel:
        text: 'ISS Tracking'
        halign: 'center'
        font_name: 'WorkSans'
        font_size: 23
        pos_hint: {'center_x':.5,'center_y':.85}


    MDIconButton:
        icon: "arrow-right"
        halign: 'center'
        pos_hint: {"center_x": .9, "center_y": .85}
        on_release:
            root.come_back_m()

