<ProfileScreen>
    name: 'profile_screen'
    BoxLayout:
        orientation: 'vertical'
        MDFloatLayout:
            md_bg_color: (1,1,1,1)
            MDFloatLayout:
                size_hint_x: 1
                size_hint_y: .33
                pos_hint: {'center_x':.5,'center_y':.85}
                canvas:
                    Color:
                        rgb: (232/255,176/255,243/255,0.5)
                    Rectangle:
                        size: self.size
                        pos: self.pos
                        #source: 
            MDFloatLayout:
                size_hint_x:.36
                size_hint_y: .2
                pos_hint: {'center_x': .5, 'center_y':.73}
                canvas:
                    Color:
                        rgb: (1,1,1,1)
                    Ellipse:
                        size: self.size
                        pos: self.pos
                        source: 'images/blank_pic.jpg'
                        angle_start: 0
                        angle_end: 360
                



                         
